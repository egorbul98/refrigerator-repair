  },
  "scripts": {
    "build:server": "webpack --config ./config/webpack.server.js --mode production",
    "build:client": "webpack --config ./config/webpack.client.js --mode production",
    "build:server:dev": "webpack --config ./config/webpack.server.js --mode development",
    "build:client:dev": "webpack --config ./config/webpack.client.js --mode development",
    "start:dev": "yarn build:client:dev && yarn build:server:dev && yarn start:server",
    "start:server": "node ./dist/server.js",
    "start": "yarn build:client && yarn build:server && yarn start:server",
    "build:css": "postcss src/styles/globals.css -o src/index.css",
    "watch:css": "postcss src/styles/globals.css -o src/index.css -w",
    "start:react-scripts": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "generate:brands": "node --loader ts-node/esm  ./scripts/generate-brands-images-imports.ts",
    "generate:certificates": "node --loader ts-node/esm  ./scripts/generate-certificates-images-imports.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
