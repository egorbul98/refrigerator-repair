{
  "name": "refrigerator-repair",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.tsx",
  "author": {
    "name": "Egor Bulakhtin",
    "email": "egorbul98@mai.ru",
    "url": "https://github.com/egorbul98"
  },
  "scripts": {
    "build:css": "postcss src/styles/globals.css -o src/index.css",
    "watch:css": "postcss src/styles/globals.css -o src/index.css -w",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "generate:brands": "node --loader ts-node/esm  ./scripts/generate-brands-images-imports.ts",
    "generate:certificates": "node --loader ts-node/esm  ./scripts/generate-certificates-images-imports.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/classnames": "^2.3.1",
    "@types/react-slick": "^0.23.10",
    "@types/swiper": "^6.0.0",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.1",
    "material-symbols": "^0.2.7",
    "react": "^18.2.0",
    "react-collapsed": "^3.3.2",
    "react-dom": "^18.2.0",
    "react-imask": "^6.4.2",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "react-yandex-maps": "^4.6.0",
    "sass": "^1.54.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^8.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-snapshot": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss-cli": "^10.0.0",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^14.1.0",
    "postcss-mixins": "^9.0.3",
    "postcss-nested": "^5.0.6",
    "postcss-reporter": "^7.0.5",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.7.1",
    "react-snap": "^1.23.0"
  }
}
